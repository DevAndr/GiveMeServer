{
  "name": "give-me-server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/give-me-server/test/jest-e2e.json",
    "prisma:studio": "npx prisma studio",
    "gen-types-graphql": "ts-node apps/give-me-server/src/generate-typings.ts",
    "prisma:gen": "prisma generate --watch",
    "dev": "concurrently \"yarn:start:dev\" \"nest start --watch parser\" \"yarn:gen-types-graphql\" \"yarn:prisma:gen\"",
    "parser": "concurrently \"nest start --watch parser\""
  },
  "dependencies": {
    "@apollo/server": "^4.10.0",
    "@bull-board/express": "^5.14.0",
    "@bull-monitor/express": "^5.4.0",
    "@golevelup/nestjs-rabbitmq": "^4.1.0",
    "@nestjs/apollo": "^12.0.11",
    "@nestjs/bull": "^10.0.1",
    "@nestjs/bullmq": "^10.0.1",
    "@nestjs/common": "10.3.1",
    "@nestjs/config": "^3.2.1",
    "@nestjs/core": "10.3.1",
    "@nestjs/graphql": "^12.0.11",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/microservices": "^10.3.1",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.1",
    "@nestjs/platform-socket.io": "^10.3.7",
    "@nestjs/websockets": "^10.3.7",
    "@prisma-korea/graphql-schema-generator": "^0.3.0",
    "@prisma/client": "^5.12.1",
    "@types/bcrypt": "^5.0.2",
    "@types/passport-jwt": "^4.0.0",
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.10.3",
    "argon2": "^0.31.2",
    "axios": "^1.6.8",
    "bull": "^4.12.2",
    "bullmq": "^5.1.5",
    "cheerio": "^1.0.0-rc.12",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "concurrently": "^8.2.2",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.4.1",
    "graphql": "^16.8.1",
    "graphql-iso-date": "^3.6.1",
    "graphql-subscriptions": "^2.0.0",
    "graphql-tools": "^9.0.0",
    "graphql-upload": "^16.0.2",
    "install": "^0.13.0",
    "ioredis": "^5.3.2",
    "joi": "^17.12.3",
    "nodemailer": "^6.9.8",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "puppeteer": "^21.9.0",
    "reflect-metadata": "^0.2.1",
    "rimraf": "^5.0.5",
    "rxjs": "^7.8.1",
    "ts-morph": "^21.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.0",
    "@nestjs/schematics": "^10.1.0",
    "@nestjs/testing": "^10.3.1",
    "@types/bull": "^4.10.0",
    "@types/cookie-parser": "^1.4.6",
    "@types/express": "^4.17.21",
    "@types/jest": "29.5.11",
    "@types/node": "^18.19.9",
    "@types/puppeteer": "^7.0.4",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.7.0",
    "prettier": "^3.2.4",
    "prisma": "^5.8.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.3.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/common(|/.*)$": "<rootDir>/libs/common/src/$1"
    }
  }
}
